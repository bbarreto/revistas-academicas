<div class="container">
  <div class="row">
    <div class="col">
      <h2 class="mb-4">{{entity.title}}</h2>
      {{#each magazines}}
      <div class="card mb-2">
        <div class="card-body">
          <h3><a href="/{{entity.id}}/{{id}}">{{title}}</a></h3>
          <p>{{description}}</p>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</div>
